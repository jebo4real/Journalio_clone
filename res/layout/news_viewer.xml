<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_outputer_id"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff">

    <ScrollView 
        android:id="@+id/sv_outputer_0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
          
    <RelativeLayout
        android:id="@+id/sv_r_outputer_0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    
    <ImageView
        android:id="@+id/iv_img_outputer"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="80dp"
        android:src="@android:drawable/ic_input_add"/>

    <TextView
        android:id="@+id/outputer_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_img_outputer"
        />
    
    <EditText
        android:id="@+id/et_outputer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_below="@+id/outputer_tv"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="34dp"
        android:ems="10"
        android:inputType="textMultiLine">

    </EditText>

	<include
	    android:id="@+id/base_bttns_parent_sv_id"
	    layout="@layout/base_bttns_sv"/>   

    
    </RelativeLayout>
    </ScrollView>
    
    <!-- C -->
    <ImageView
        android:id="@+id/iv_img_cancel"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:src="@android:drawable/ic_delete"/>
    
    
    
</RelativeLayout>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amalitech.journalio_clone"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="24" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
         <activity
            android:name="com.amalitech.journalio_clone.Activity_MainNewsPage"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.amalitech.journalio_clone.Activity_Inputer"
            android:label="@string/app_name">
        </activity>
        
        <activity
            android:name="com.amalitech.journalio_clone.Activity_Poster"
            android:label="@string/app_name">
        </activity>
        
    </application>

</manifest>
